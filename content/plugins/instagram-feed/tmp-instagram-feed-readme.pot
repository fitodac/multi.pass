#. Found in installation list item.
msgid "Install the Instagram Feed plugin either via the WordPress plugin directory, or by uploading the files to your web server (in the <code>/wp-content/plugins/</code> directory)."
msgstr ""

#. Found in installation list item.
msgid "Activate the Instagram Feed plugin through the 'Plugins' menu in WordPress."
msgstr ""

#. Found in installation list item.
msgid "Navigate to the 'Instagram Feed' settings page to obtain your Instagram Access Token and Instagram User ID and configure your settings."
msgstr ""

#. Found in installation list item.
msgid "Use the shortcode <code>[instagram-feed]</code> in your page, post or widget to display your Instagram photos."
msgstr ""

#. Found in installation list item.
msgid "You can display multiple Instagram feeds by using shortcode options, for example: <code>[instagram-feed num=6 cols=3]</code>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>General Options</strong>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>id</strong> - An Instagram User ID - Example: <code>[instagram-feed id=AN_INSTAGRAM_USER_ID]</code>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>width</strong> - The width of your Instagram feed. Any number - Example: <code>[instagram-feed width=50]</code>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>widthunit</strong> - The unit of the width of your Instagram feed. 'px' or '%' - Example: <code>[instagram-feed widthunit=%]</code>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>height</strong> - The height of your Instagram feed. Any number - Example: <code>[instagram-feed height=250]</code>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>heightunit</strong> - The unit of the height of your Instagram feed. 'px' or '%' - Example: <code>[instagram-feed heightunit=px]</code>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>background</strong> - The background color of the Instagram feed. Any hex color code - Example: <code>[instagram-feed background=#ffff00]</code>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>class</strong> - Add a CSS class to the Instagram feed container - Example: <code>[instagram-feed class=feedOne]</code>"
msgstr ""



#. Found in installation list item, faq list item.
msgid "<strong>Photo Options</strong>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>sortby</strong> - Sort the Instagram posts by Newest to Oldest (none) or Random (random) - Example: <code>[instagram-feed sortby=random]</code>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<p><strong>num</strong> - The number of Instagram posts to display initially. Maximum is 33 - Example: <code>[instagram-feed num=10]</code></p>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<p><strong>cols</strong> - The number of columns in your Instagram feed. 1 - 10 - Example: <code>[instagram-feed cols=5]</code></p>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>imageres</strong> - The resolution/size of the Instagram photos. 'auto', full', 'medium' or 'thumb' - Example: <code>[instagram-feed imageres=full]</code>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>imagepadding</strong> - The spacing around your Instagram photos - Example: <code>[instagram-feed imagepadding=10]</code>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>imagepaddingunit</strong> - The unit of the padding in your Instagram feed. 'px' or '%' - Example: <code>[instagram-feed imagepaddingunit=px]</code>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>disablemobile</strong> - Disable the mobile layout for your Instagram feed. 'true' or 'false' - Example: <code>[instagram-feed disablemobile=true]</code>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>Header Options</strong>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>showheader</strong> - Whether to show the Instagram feed Header. 'true' or 'false' - Example: <code>[instagram-feed showheader=false]</code>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>headercolor</strong> - The color of the Instagram feed Header text. Any hex color code - Example: <code>[instagram-feed headercolor=#333]</code>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>'Load More' Button Options</strong>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>showbutton</strong> - Whether to show the 'Load More' button. 'true' or 'false' - Example: <code>[instagram-feed showbutton='false']</code>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>buttoncolor</strong> - The background color of the button. Any hex color code - Example: <code>[instagram-feed buttoncolor=#000]</code>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>buttontextcolor</strong> - The text color of the button. Any hex color code - Example: <code>[instagram-feed buttontextcolor=#fff]</code>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>buttontext</strong> - The text used for the button - Example: <code>[instagram-feed buttontext=\"Load More Photos\"]</code>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>'Follow on Instagram' Button Options</strong>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>showfollow</strong> - Whether to show the 'Follow on Instagram' button. 'true' or 'false' - Example: <code>[instagram-feed showfollow=true]</code>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>followcolor</strong> - The background color of the 'Follow on Instagram' button. Any hex color code - Example: <code>[instagram-feed followcolor=#ff0000]</code>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>followtextcolor</strong> - The text color of the 'Follow on Instagram' button. Any hex color code - Example: <code>[instagram-feed followtextcolor=#fff]</code>"
msgstr ""

#. Found in installation list item, faq list item.
msgid "<strong>followtext</strong> - The text used for the 'Follow on Instagram' button - Example: <code>[instagram-feed followtext=\"Follow me\"]</code>"
msgstr ""

