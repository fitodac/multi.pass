    }
    a {
      border: none;
      color: #fff;
      display: block;
      font-weight: 300;
      padding: 10px 20px;
      padding: 1rem 2rem;
      position: relative;
      &:hover {
        background-color: #fff;
        color: #000;
      }
    }
    .current-menu-item > a {
      background-color: #fff;
      color: #000;
    }
    .sub-menu {
      a, .current-menu-item > a:hover {
        background-color: #fff;
        color: #000;
      }
      a {
        &:hover {
          background-color: #000;
          color: #fff;
        }
        background-color: #fff;
        border: 1px solid #eee;
        border-top: none;
        font-size: 14px;
        font-size: 1.4rem;
        padding: 10px 20px;
        padding: 1rem 2rem;
        position: relative;
        width: 180px;
      }
      left: -9999px;
      margin-left: -1px;
      margin-left: -0.1rem;
      opacity: 0;
      position: absolute;
      -webkit-transition: opacity .4s ease-in-out;
      -moz-transition: opacity .4s ease-in-out;
      -ms-transition: opacity .4s ease-in-out;
      -o-transition: opacity .4s ease-in-out;
      transition: opacity .4s ease-in-out;
      width: 180px;
      z-index: 99;
      .sub-menu {
        margin: -42px 0 0 179px;
      }
    }
    .menu-item:hover {
      position: static;
      > .sub-menu {
        left: auto;
        opacity: 1;
      }
    }
    > {
      .right {
        color: #fff;
        float: right;
        list-style-type: none;
        display: inline-block;
        a {
          display: inline-block;
        }
        > a:hover {
          background: none;
          color: #aaa;
        }
        &.date {
          padding: 10px 20px;
          padding: 1rem 2rem;
        }
      }
      .search {
        margin-right: 20px;
        margin-right: 2rem;
        padding: 1px;
        padding: 0.1rem;
      }
    }
  }
